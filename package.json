  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:pug": "pug src/pug --out dist/",
    "build:scss": "node-sass -r src/scss/ -o dist/css/",
    "clean": "rimraf dist/css/*.css dist/html",
    "deploy": "ghpages --path dist",
    "prebuild": "npm run clean",
    "prewatch": "npm run clean",
    "watch": "run-p watch:*",
    "watch:pug": "pug -w src/pug --out dist",
    "watch:scss": "node-sass -r -w src/scss/ -o dist/css/"
  },
